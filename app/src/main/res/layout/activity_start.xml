            app:layout_constraintEnd_toEndOf="@+id/imageRectangle1"
            app:layout_constraintStart_toStartOf="@+id/imageRectangle1"
            app:layout_constraintTop_toTopOf="@+id/imageRectangle1"
            style="@style/txtInterbold28" tools:text="@string/lbl_get_started"/>
        <ImageView android:id="@+id/imageRectangle2"
            android:layout_height="@dimen/_160pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_20pxv"
            android:layout_width="@dimen/_180pxh"
            android:scaleType="fitXY"
            app:layout_constraintEnd_toStartOf="@+id/viewEllipse4"
            app:layout_constraintHorizontal_bias="0.31"
            app:layout_constraintStart_toStartOf="@+id/viewEllipse1"
            app:layout_constraintTop_toTopOf="@+id/imageVector2"
            imageRes="@{@drawable/img_rectangle2}" tools:src="@drawable/img_rectangle2"/>
        <ImageView android:id="@+id/imageGirlandboysi"
            android:layout_height="0dp"
            android:layout_marginStart="@dimen/_39pxh"
            android:layout_marginTop="@dimen/_43pxv"
            android:layout_width="0dp" android:scaleType="fitXY"
            app:layout_constraintBottom_toTopOf="@+id/txtAnswerTheQuiz"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHeight_percent="0.64"
            app:layout_constraintStart_toStartOf="@+id/viewEllipse2"
            app:layout_constraintTop_toTopOf="@+id/viewEllipse1"
            app:layout_constraintWidth_percent="0.87439615"
            imageRes="@{@drawable/img_girlandboysi}" tools:src="@drawable/img_girlandboysi"/>
        <TextView android:ellipsize="end" android:id="@+id/txtWelcomeTo"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_35pxh"
            android:layout_marginStart="@dimen/_52pxh"
            android:layout_marginTop="@dimen/_143pxv"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@{startVM.startModel.txtWelcomeTo}"
            app:layout_constraintEnd_toEndOf="@+id/txtQUIZAPP"
            app:layout_constraintHorizontal_bias="0.13"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageVector2"
            style="@style/txtInterbold24" tools:text="@string/lbl_welcome_to"/>

        <TextView
            android:id="@+id/txtQUIZAPP"
            style="@style/txtInterbold32"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_52pxh"
            android:layout_marginEnd="@dimen/_165pxh"
            android:layout_marginBottom="@dimen/_7pxv"
            android:ellipsize="end"
            android:singleLine="true"
            android:translationX="-24dp"
            android:text="@{startVM.startModel.txtQUIZAPP}"
            app:layout_constraintBottom_toBottomOf="@+id/imageGirlandboysi"
            app:layout_constraintEnd_toEndOf="@+id/imageGirlandboysi"
            app:layout_constraintHorizontal_bias="0.49"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/txtWelcomeTo"
            app:layout_constraintVertical_bias="0.416"
            tools:text="@string/lbl_quiz_app" />

        <TextView android:gravity="start"
            android:id="@+id/txtAnswerTheQuiz"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_12pxh"
            android:layout_marginStart="@dimen/_52pxh"
            android:layout_marginTop="@dimen/_3pxv"
            android:layout_width="0dp"
            android:text="@{startVM.startModel.txtAnswerTheQuiz}"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageGirlandboysi"
            style="@style/txtInterregular16" tools:text="@string/msg_answer_the_quiz"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
